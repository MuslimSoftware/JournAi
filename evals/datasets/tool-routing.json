{
  "version": "1.0",
  "moduleId": "tool-router",
  "description": "Golden dataset for tool routing decisions - demonstrates expected tool calls and arguments",
  "examples": [
    {
      "id": "tool-search-001",
      "input": {
        "query": "What did I write about my morning routine?",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": true,
        "toolName": "search_journal",
        "toolArguments": { "query": "morning routine" }
      },
      "metadata": { "category": "search", "difficulty": "easy" }
    },
    {
      "id": "tool-search-002",
      "input": {
        "query": "Show me entries about work stress",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": true,
        "toolName": "search_journal",
        "toolArguments": { "query": "work stress" }
      },
      "metadata": { "category": "search", "difficulty": "easy" }
    },
    {
      "id": "tool-insights-001",
      "input": {
        "query": "What emotions have I been feeling lately?",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": true,
        "toolName": "get_insights",
        "toolArguments": { "type": "emotions" }
      },
      "metadata": { "category": "insights", "difficulty": "easy" }
    },
    {
      "id": "tool-insights-002",
      "input": {
        "query": "Who have I been spending time with?",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": true,
        "toolName": "get_insights",
        "toolArguments": { "type": "people" }
      },
      "metadata": { "category": "insights", "difficulty": "easy" }
    },
    {
      "id": "tool-date-001",
      "input": {
        "query": "What did I write this week?",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": true,
        "toolName": "get_entries_by_date",
        "toolArguments": {
          "startDate": "2025-01-07",
          "endDate": "2025-01-14"
        }
      },
      "metadata": { "category": "date", "difficulty": "medium" }
    },
    {
      "id": "tool-date-002",
      "input": {
        "query": "Show me last month's entries",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": true,
        "toolName": "get_entries_by_date",
        "toolArguments": {
          "startDate": "2024-12-15",
          "endDate": "2025-01-14"
        }
      },
      "metadata": { "category": "date", "difficulty": "medium" }
    },
    {
      "id": "tool-none-001",
      "input": {
        "query": "Hi! How are you?",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": false,
        "toolName": null,
        "toolArguments": null
      },
      "metadata": { "category": "no-tool", "difficulty": "easy" }
    },
    {
      "id": "tool-none-002",
      "input": {
        "query": "Thanks for your help!",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": false,
        "toolName": null,
        "toolArguments": null
      },
      "metadata": { "category": "no-tool", "difficulty": "easy" }
    },
    {
      "id": "tool-date-003",
      "input": {
        "query": "What did I write on January 5th?",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": true,
        "toolName": "get_entries_by_date",
        "toolArguments": {
          "startDate": "2025-01-05",
          "endDate": "2025-01-05"
        }
      },
      "metadata": { "category": "date", "difficulty": "easy" }
    },
    {
      "id": "tool-insights-003",
      "input": {
        "query": "Where have I traveled recently?",
        "availableTools": ["search_journal", "get_insights", "get_entries_by_date"],
        "currentDate": "2025-01-14"
      },
      "expectedOutput": {
        "shouldUseTool": true,
        "toolName": "get_insights",
        "toolArguments": { "type": "locations" }
      },
      "metadata": { "category": "insights", "difficulty": "easy" }
    }
  ]
}
